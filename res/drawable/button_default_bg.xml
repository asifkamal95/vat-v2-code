<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_pressed="true" >
        <shape>
            <solid
                android:color="#69D5A3" />
            <stroke
                android:width="0.5dp"
                android:color="#69D5A3" />
            <corners
                android:radius="5dp" />

        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" android:padding="10dp">
            <gradient
                android:startColor="#00B863"
                android:endColor="#00B863"
                android:angle="-90" />
           <stroke
        android:width="0.5dp"
        android:color="#00B863" />
     <corners
         android:radius="5dp"
         />

        </shape>
    </item>
</selector>